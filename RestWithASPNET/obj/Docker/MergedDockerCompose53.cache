name: dockercompose3995615439486361525
services:
  db:
    build:
      context: D:\WorkSpace\RestWithAsp\RestWithAsp\RestWithASPNET
      dockerfile: db/Dockerfile
    command:
    - mysqld
    - --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: rest_with_asp_net
      MYSQL_PASSWORD: MainPassword
      MYSQL_ROOT_PASSWORD: RootPassword
      MYSQL_USER: MainUser
      TZ: America/Sao_Paulo
    image: rest-with-asp-net-db
    networks:
      api-network: null
    ports:
    - mode: ingress
      target: 3306
      published: "3308"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      target: /var/lib/mysql
      volume: {}
  rest-with-asp-net:
    build:
      context: D:\WorkSpace\RestWithAsp\RestWithAsp\RestWithASPNET
      dockerfile: RestWithASPNET/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RestWithASPNET
      target: base
    container_name: RestWithASPNET
    depends_on:
      db:
        condition: service_started
    entrypoint:
    - tail
    - -f
    - /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
      TZ: America/Sao_Paulo
    image: rest-with-asp-net:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/RestWithASPNET.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      api-network: null
    ports:
    - mode: ingress
      target: 80
      published: "44386"
      protocol: tcp
    tty: true
    volumes:
    - type: bind
      source: D:\WorkSpace\RestWithAsp\RestWithAsp\RestWithASPNET\RestWithASPNET
      target: /app
      bind:
        create_host_path: true
    - type: bind
      source: D:\WorkSpace\RestWithAsp\RestWithAsp\RestWithASPNET
      target: /src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\kinkar\vsdbg\vs2017u5
      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\kinkar\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
      target: /root/.nuget/fallbackpackages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\kinkar\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\kinkar\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  api-network:
    name: dockercompose3995615439486361525_api-network
    driver: bridge