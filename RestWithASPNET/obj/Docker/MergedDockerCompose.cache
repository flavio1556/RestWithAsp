name: dockercompose3995615439486361525
services:
  db:
    build:
      context: D:\WorkSpace\RestWithAsp\RestWithAsp\RestWithASPNET
      dockerfile: db/Dockerfile
    command:
    - mysqld
    - --default-authentication-plugin=mysql_native_password
    environment:
      MYSQL_DATABASE: rest_with_asp_net
      MYSQL_PASSWORD: MainPassword
      MYSQL_ROOT_PASSWORD: RootPassword
      MYSQL_USER: MainUser
      TZ: America/Sao_Paulo
    image: rest-with-asp-net-db
    networks:
      api-network: null
    ports:
    - mode: ingress
      target: 3306
      published: "3308"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      target: /var/lib/mysql
      volume: {}
  rest-with-asp-net:
    build:
      context: D:\WorkSpace\RestWithAsp\RestWithAsp\RestWithASPNET
      dockerfile: RestWithASPNET/Dockerfile
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      TZ: America/Sao_Paulo
    image: rest-with-asp-net
    networks:
      api-network: null
    ports:
    - mode: ingress
      target: 80
      published: "44386"
      protocol: tcp
networks:
  api-network:
    name: dockercompose3995615439486361525_api-network
    driver: bridge